{
  "name": "babylonjs-editor",
  "version": "2.0.0",
  "description": "Babylon.js Editor is a Web Application helping artists to work with Babylon.js",
  "main": ".build/electron/main.js",
  "repository": {
    "url": "https://github.com/BabylonJS/Editor/"
  },
  "scripts": {
    "postinstall": "node ./script/dts.js",
    "clean": "rm -rf .build && rm -rf .declaration",
    "compile": "tsc -p .",
    "compile-electron": "tsc -p ./electron",
    "build": "npm run clean && npm run compile && npm run compile-electron && npm run bundle",
    "watch": "tsc -p . --watch",
    "watch-electron": "tsc -p ./electron --watch",
    "webserver": "http-server -p 1338",
    "bundle": "node ./script/dts.js && node ./script/bundle.js && node ./script/electron.js"
  },
  "readme": "https://github.com/BabylonJS/Editor/blob/master/README.md",
  "license": "(Apache-2.0)",
  "devDependencies": {
    "@types/node": "8.0.47",
    "@types/w2ui": "1.4.32",
    "@types/dat-gui": "0.6.3",
    "@types/systemjs": "0.20.6",
    "@types/raphael": "2.1.30",
    "@types/electron": "1.6.10",
    "@types/http-server": "0.10.0",
    "@types/socket.io": "1.4.31",
    "@types/koa": "~2.0.40",
    "@types/koa-router": "~7.0.25",
    "@types/fs-extra": "~4.0.5",
    "@types/socket.io-client": "~1.4.32",
    "typescript": "^2.3.3",
    "http-server": "^0.9.0",
    "socket.io": "2.0.4",
    "electron": "1.7.9",
    "electron-prebuilt": "~1.4.13",
    "electron-packager": "~8.6.0",
    "yargs": "10.0.3",
    "systemjs-builder": "~0.16.12",
    "dts-bundle": "~0.7.3"
  },
  "dependencies": {
    "jquery": "~3.2.1",
    "dat.gui": "~0.6.1",
    "dat-gui": "0.5.0",
    "systemjs-plugin-css": "~0.1.35",
    "w2ui": "~1.4.3",
    "systemjs": "~0.20.19",
    "es6-promise": "~4.1.0",
    "babylonjs": "3.1.0-beta5",
    "babylonjs-gui": "3.1.0-beta5",
    "babylonjs-loaders": "3.1.0-beta5",
    "babylonjs-materials": "3.1.0-beta5",
    "spectorjs": "~0.9.2",
    "cannon": "~0.6.2",
    "monaco-editor": "~0.5.3",
    "raphael": "~2.2.7",
    "socket.io-client": "~2.0.4",
    "koa": "~2.4.1",
    "koa-router": "~7.3.0",
    "koa-socket": "~4.4.0",
    "fs-extra": "~4.0.2"
  }
}
