{
  "name": "babylonjs-editor",
  "version": "0.0.0",
  "description": "Babylon.js Editor is a Web Application helping artists to work with Babylon.js",
  "main": ".build/electron/main.js",
  "productName": "Babylon.js Editor",
  "repository": {
    "url": "https://github.com/BabylonJS/Editor/"
  },
  "scripts": {
    "clean": "rimraf .build && rimraf .declaration && rimraf dist && rimraf electron-packages",
    "compile": "tsc -p .",
    "compile-electron": "tsc -p ./electron",
    "build": "npm run compile && npm run compile-electron && npm run bundle",
    "rebuild": "npm run clean && npm run compile && npm run compile-electron && npm run bundle",
    "watch": "tsc -p . --watch",
    "watch-electron": "tsc -p ./electron --watch",
    "webserver": "http-server -p 1338",
    "bundle": "node ./script/dts.js && node ./script/bundle.js && node ./script/electron.js",
    "bundle-js": "node ./script/bundle.js",
    "installer-osx": "node ./script/electron.js --platform=darwin",
    "installer-win32": "node ./script/electron.js --platform=win32",
    "dts": "node ./script/dts.js"
  },
  "readme": "https://github.com/BabylonJS/Editor/blob/master/README.md",
  "license": "(Apache-2.0)",
  "devDependencies": {
    "@types/dat-gui": "~0.6.3",
    "@types/electron": "~1.6.10",
    "@types/fs-extra": "~4.0.5",
    "@types/http-server": "~0.10.0",
    "@types/jstree": "^3.3.36",
    "@types/koa": "~2.0.40",
    "@types/koa-router": "~7.0.25",
    "@types/node": "~8.0.47",
    "@types/raphael": "~2.1.30",
    "@types/socket.io": "~1.4.31",
    "@types/socket.io-client": "~1.4.32",
    "@types/systemjs": "~0.20.6",
    "@types/w2ui": "~1.4.32",
    "dts-bundle": "~0.7.3",
    "electron": "~1.7.9",
    "electron-installer-dmg": "~0.2.1",
    "electron-packager": "~10.1.1",
    "electron-prebuilt": "~1.4.13",
    "electron-winstaller": "~2.6.3",
    "http-server": "^0.9.0",
    "rimraf": "~2.6.2",
    "socket.io": "~2.0.4",
    "systemjs-builder": "~0.16.12",
    "typescript": "~2.6.2",
    "yargs": "10.0.3"
  },
  "dependencies": {
    "babylonjs": "~3.2.0-alphaB",
    "babylonjs-gui": "~3.2.0-alphaB",
    "babylonjs-loaders": "~3.2.0-alphaB",
    "babylonjs-materials": "~3.2.0-alphaB",
    "babylonjs-procedural-textures": "~3.2.0-alphaB",
    "babylonjs-serializers": "~3.2.0-alphaB",
    "cannon": "~0.6.2",
    "dat-gui": "~0.5.0",
    "dat.gui": "~0.7.0",
    "es6-promise": "~4.1.0",
    "fs-extra": "~4.0.2",
    "golden-layout": "~1.5.9",
    "jquery": "~3.2.1",
    "jstree": "^3.3.5",
    "koa": "~2.4.1",
    "koa-router": "~7.3.0",
    "koa-socket": "~4.4.0",
    "monaco-editor": "~0.10.1",
    "raphael": "~2.2.7",
    "socket.io-client": "~2.0.4",
    "spectorjs": "~0.9.2",
    "systemjs": "~0.20.19",
    "systemjs-plugin-css": "~0.1.35",
    "w2ui": "~1.4.3"
  }
}
