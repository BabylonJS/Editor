    },
    "scripts": {
        "build": "tsc -p . && tailwindcss -i ./index.css -o ./build/index.css",
        "watch": "tsc -p . --watch",
        "watch-css": "tailwindcss -i ./index.css -o ./build/index.css --watch",
        "watch-all": "concurrently \"yarn watch\" \"yarn watch-css\" -c bgYellow.bold,bgBlue.bold --names editor-ts,editor-css"
    },
